{-
Funcion: Creditos
Descripcion: Recibe un entero y devuelve la materia o materias que tienen ese mismo numero de creditos
Uso: creditos 10 [Introduccion a las CC]
-}
let Materias = [("Introduccion a Ciencias de la computacion", 12),("Algebra Superior 1", 10),("Estructuras Discretas", 10),("Matematicas para las Ciencias Aplicadas 1", 12),("Ingles 1", 4)] 


creditos :: Int -> Materias -> Maybe a
creditos a = [ a = (_,a)  | (a) <- [Materias]]
